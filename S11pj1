      "source": [
        "import numpy as np\n",
        "import scipy.signal as signal\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# مشخصات فیلتر\n",
        "fs = 1000  # فرکانس نمونه‌برداری (Hz)\n",
        "fc = 100  # فرکانس قطع (Hz)\n",
        "order = 4  # مرتبه فیلتر\n",
        "\n",
        "# طراحی فیلتر آنالوگ (باترورث)\n",
        "analog_filter = signal.butter(order, fc, btype='low', analog=True)\n",
        "\n",
        "# تبدیل فیلتر آنالوگ به دیجیتال با استفاده از تبدیل دو خطی\n",
        "digital_filter = signal.bilinear(analog_filter[0], analog_filter[1], fs)\n",
        "\n",
        "# محاسبه پاسخ فرکانسی فیلتر دیجیتال\n",
        "w, h = signal.freqz(digital_filter[0], digital_filter[1], worN=8000)\n",
        "\n",
        "# رسم پاسخ فرکانسی\n",
        "plt.figure()\n",
        "plt.plot(0.5 * fs * w / np.pi, np.abs(h), 'b')\n",
        "plt.plot(fc, 0.5 * np.sqrt(2), 'ko')\n",
        "plt.axvline(fc, color='k')\n",
        "plt.xlim(0, 0.5 * fs)\n",
        "plt.title(\"پاسخ فرکانسی فیلتر دیجیتال\")\n",
        "plt.xlabel(\"فرکانس (Hz)\")\n",
        "plt.grid()\n",
        "plt.show()\n",
        "\n",
        "# محاسبه پاسخ ضربه فیلتر دیجیتال\n",
        "impulse = signal.unit_impulse(100)\n",
        "response = signal.lfilter(digital_filter[0], digital_filter[1], impulse)\n",
        "\n",
        "# رسم پاسخ ضربه\n",
        "plt.figure()\n",
        "plt.stem(response)\n",
        "plt.title(\"پاسخ ضربه فیلتر دیجیتال\")\n",
        "plt.xlabel(\"نمونه\")\n",
        "plt.grid()\n",
        "plt.show()"
      ]
    },
